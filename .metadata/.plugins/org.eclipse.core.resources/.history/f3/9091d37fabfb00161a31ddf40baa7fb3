#include <xs1.h>
#include <platform.h>
#include <stdio.h>

#include "ov07740.h"
#include "io.h"


// "UI" tile
void Tile0()
{

}

// "Camera" tile
void Tile1()
{ unsafe {

    printf("Booting AutoUmp...\n");

    initCams();
    configureCams();
    launchCameras();
}}

int main()
{
    streaming chan bluetoothChan;

    par
    {
        on tile[0]: Tile0();
        on tile[1]: Tile1();
    }
    return 0;
}
