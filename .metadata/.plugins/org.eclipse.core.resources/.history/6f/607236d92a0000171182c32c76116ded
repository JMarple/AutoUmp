#include <xs1.h>
#include <platform.h>
#include <stdlib.h>
#include <stdio.h>
#include <stdint.h>

#include "algs.h"

void floodFill(chanend stream)
{
    while (1==1)
    {
        uint32_t starttime, endtime;
        uint8_t* unsafe bitBuffer;
        timer t;

        t :> starttime;

        // Recieves the buffer
        stream :> bitBuffer;
        // Tim your code goes here.

        t :> endtime;
        printf("Clock ticks (@100Mhz) = %d\n", (endtime - starttime));
    }
}
